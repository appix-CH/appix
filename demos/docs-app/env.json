{
  "aliases": {
    "components": "@{appPath}/components",
    "models": "@{appPath}/models",
    "views": "@{appPath}/views",
    "filters": "@{appPath}/filters",
    "assets": "@{appPath}/../assets"
  },
  "components": {
    "cache": {
      "filePath": "@{components}/memory-cache"
    },
    "asset-reader": {
      "filePath": "@{components}/asset-reader"
    },
    "view": {
      "filePath": "@{components}/view"
    },
    "appix/logger": {
      "enabled": true,
      "console": true,
      "level": 10
    },
    "appix/router": {
      "useCustomErrorHandler": false
    }
  }
}